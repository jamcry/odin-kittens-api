<h1>Kittens</h1>
<% @kittens.each do |kitten| %>
  <p>
    <%= kitten.name %> aged <%= kitten.age %> |
    <%= link_to "Show", kitten_path(kitten) %> |
    <%= link_to "Edit", edit_kitten_path(kitten) %> |
    <%= link_to "Delete", kitten_path(kitten), method: :delete,
        data: { confirm: "You are deleting a kitten (#{kitten.name})!"} %>
  </p>
<% end %>

<%= link_to "Back", :back %>
<%= link_to "New", new_kitten_path %>